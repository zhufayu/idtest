DROP TABLE IF EXISTS `wm_order_ware_00`;
 CREATE TABLE `wm_order_ware_00` (
  `id` bigint(20) NOT NULL,
  `order_id` bigint(50) DEFAULT NULL,
  `shop_id` bit(20) DEFAULT NULL COMMENT '商家内部门店编号（废弃）',
  `sku_id` bigint(20) DEFAULT NULL,
  `wm_sku_id` tinyint(1) unsigned zerofill DEFAULT NULL COMMENT '物美商品sku',
  `ware_id` smallint(20) DEFAULT NULL,
  `wm_ware_id` mediumint(20) unsigned zerofill DEFAULT NULL COMMENT '物美商品id',
  `matnr` double DEFAULT NULL,
  `ware_weight` float DEFAULT 0,
  `ware_name` varchar(200) DEFAULT '',
  `ware_img_url` varchar(500) DEFAULT NULL,
  `item_num` varchar(50) DEFAULT '0',
  `ware_num` int(8) DEFAULT NULL,
  `offline_num` decimal(12,4) DEFAULT NULL COMMENT '线下推送订单的商品数量(支持浮点数,可能是0.6千克，可以精确到0.0001kg)',
  `ware_type` int(4) DEFAULT NULL COMMENT '商品类型\\n1：普通\\n2：赠品\\n3：附件',
  `ware_price` bigint(12) DEFAULT NULL COMMENT '商品价格',
  `promotion_price` decimal(12,0) DEFAULT 0 COMMENT '促销优惠',
  `promotion_id` varchar(1000) DEFAULT NULL COMMENT '该商品参与的所有促销ID，多个值逗号隔开',
  `properties` varchar(2000) DEFAULT NULL COMMENT '订单商品扩展字段',
  `created` datetime DEFAULT NULL,
  `modified` datetime DEFAULT NULL,
  `yn` tinyint(255) DEFAULT NULL,
  `sku_type` tinyint(100) DEFAULT NULL COMMENT '商品类型(1:标品,2:散卖)',
  `promotion_price_list` varchar(1000) DEFAULT NULL COMMENT '该商品参与的所有促销金额，多个字用(,)隔开',
  `promotion_type` decimal(10,0) DEFAULT NULL COMMENT '该商品参与的所有促销类型，多个类型用逗号(,)隔开',
  `coupon_amount` decimal(8,0) DEFAULT NULL COMMENT '优惠券平分到每个商品上面的金额',
  `invoice_type` decimal(50,0) DEFAULT NULL COMMENT '发票类型',
  `source_ware_id` decimal(20,0) DEFAULT NULL COMMENT '修改订单中，本商品记录在原订单商品表中的ID',
  `webuser_id` char(200) DEFAULT NULL,
  `coupon_code_amount` tinytext DEFAULT NULL COMMENT '优惠码分摊到每个商品上的金额',
  `catagory_id` text DEFAULT NULL COMMENT '商品品类ID',
  `ware_tag` mediumtext DEFAULT NULL COMMENT '商品标签，以二进制位表示的整数',
  `custom_tag` longtext DEFAULT NULL COMMENT '商品自定义扩展标记',
  `matkl` tinyblob DEFAULT NULL COMMENT '商品线下品类id，多个用逗号隔开',
  `offline_bar_code` varbinary(100) DEFAULT NULL COMMENT '商品条形码，多个用逗号隔开',
  `promotion_sub_type` enum('RED','GREEN','BLUE') DEFAULT NULL COMMENT '促销子类型',
  `ware_promotion_price` set('a','b','c','d') DEFAULT NULL COMMENT '商品促销后的单价',
  `offline_coupon_amount` bigint(20) DEFAULT 0,
  `dmall_coupon_detail` geometrycollection DEFAULT NULL COMMENT '多点券使用明细',
  `offline_coupon_detail` geometry DEFAULT NULL COMMENT '商家券使用明细',
  `lucky_cut_detail` point DEFAULT NULL COMMENT '随机减使用明细',
  `promotion_detail` multipoint DEFAULT NULL COMMENT '促销优惠明细',
  `payment_detail` polygon DEFAULT NULL COMMENT '商品支付明细',
  `MultiLineStringtest` multilinestring DEFAULT NULL,
  `LineStringtest` linestring DEFAULT NULL,
  `bianyrtest` binary(100) DEFAULT NULL,
  `dmall_coupon_detail_new` longtext CHARACTER SET utf8mb4 COLLATE utf8mb4_bin DEFAULT NULL COMMENT '多点券使用明细新',
  `dmall_coupon_detailCopy` varchar(500) DEFAULT NULL COMMENT '多点券使用明细',
  `timetest` time DEFAULT NULL,
  `datatest` date DEFAULT NULL,
  `yeartest` year(4) DEFAULT NULL,
  `timestamptest` timestamp NULL DEFAULT NULL,
  `blobtest` blob DEFAULT NULL,
  `meblob` mediumblob DEFAULT NULL,
  `longblobtest` longblob DEFAULT NULL,
  `longblobtestCopy` longblob DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `order_id_idx` (`order_id`),
  KEY `idx_created` (`created`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


